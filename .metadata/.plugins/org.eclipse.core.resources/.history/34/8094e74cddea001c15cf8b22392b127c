package oop;

public class NewStudent {
	
	private static String id="asdfgh";
	int phone;
	String city;
	String state;
	
	public int getPhone() {
		return phone;
	}

	public void setPhone(int phone) {
		this.phone = phone;
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

	public String getState() {
		return state;
	}

	public void setState(String state) {
		this.state = state;
	}

	NewStudent(String name, int SSN){
		String id=name+"@ksu.edu";
	}
}
