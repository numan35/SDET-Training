package training1;

public class loops {

	public static void main(String[] args) {
		
		
		
		String[] states=new String[5];
		states[0]="New York";
		states[1]="Georgia";
		states[2]="Florida";
		states[3]="Texas";
		states[4]="California";
		
		// Do loop: enters the loop THEN tests condition
		
		int i=0;
		
		do {
			System.out.println(states[i]);
		}while(i<5)
		
		/*for(int i=0; i<5; i++) {
	
			System.out.println(states[i]);
*/
	}

}
